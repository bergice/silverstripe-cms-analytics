{
    "name": "maximerainville/silverstripe-cms-analytics",
    "description": "Add analytics tracking to the Silverstripe CMS.",
    "type": "silverstripe-vendormodule",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxime Rainville",
            "email": "maxime@silverstripe.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "silverstripe/admin": "^1.0",
        "silverstripe/vendor-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "MaximeRainville\\SilverstripeCmsAnalytics\\": "code/",
            "MaximeRainville\\SilverstripeCmsAnalytics\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "phpcs code/ tests/php/",
        "lint-clean": "phpcbf code/ tests/php/"
    },
    "prefer-stable": true
}
